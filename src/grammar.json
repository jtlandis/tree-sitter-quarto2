        }
      ]
    },
    "heading_3": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "###"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "heading_4": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "####"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "heading_5": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#####"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "heading_6": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "######"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "emph": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
