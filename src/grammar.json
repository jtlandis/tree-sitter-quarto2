{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "quarto",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_section"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "<!--"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "STRING",
            "value": "-->"
          }
        ]
      }
    },
    "_yaml": {
      "type": "CHOICE",
      "members": []
    },
    "paragraph": {
      "type": "PREC_RIGHT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_line"
            }
          },
          {
            "type": "SYMBOL",
            "name": "paragraph_end"
          }
        ]
      }
    },
    "line_break": {
      "type": "PREC_RIGHT",
      "value": 2,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "\\"
                }
              },
              {
                "type": "SYMBOL",
                "name": "line_end"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "  "
                }
              },
              {
                "type": "SYMBOL",
                "name": "line_end"
              }
            ]
          }
        ]
      }
    },
    "paragraph_end": {
      "type": "PREC_RIGHT",
      "value": 4,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "line_end"
        }
      }
    },
    "_line_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "emph"
          },
          {
            "type": "SYMBOL",
            "name": "word"
          },
          {
            "type": "SYMBOL",
            "name": "puncuation"
          },
          {
            "type": "SYMBOL",
            "name": "literal"
          },
          {
            "type": "SYMBOL",
            "name": "symbols"
          }
        ]
      }
    },
    "_line": {
      "type": "PREC_RIGHT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_line_content"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "line_break"
              },
              {
                "type": "SYMBOL",
                "name": "line_end"
              }
            ]
          }
        ]
      }
    },
    "word": {
      "type": "PATTERN",
      "value": "[\\p{L}\\p{N}]+[\\p{L}\\p{N}_]*"
    },
    "puncuation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "period"
        },
        {
          "type": "SYMBOL",
          "name": "comma"
        },
        {
          "type": "SYMBOL",
          "name": "question"
        },
        {
          "type": "SYMBOL",
          "name": "exclamation"
        },
        {
          "type": "SYMBOL",
          "name": "colon"
        },
        {
          "type": "SYMBOL",
          "name": "semi_colon"
        },
        {
          "type": "SYMBOL",
          "name": "quotation"
        }
      ]
    },
    "period": {
      "type": "STRING",
      "value": "."
    },
    "comma": {
      "type": "STRING",
      "value": ","
    },
    "exclamation": {
      "type": "STRING",
      "value": "!"
    },
    "question": {
      "type": "STRING",
      "value": "?"
    },
    "colon": {
      "type": "STRING",
      "value": ":"
    },
    "semi_colon": {
      "type": "STRING",
      "value": ";"
    },
    "quotation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_quote"
        },
        {
          "type": "SYMBOL",
          "name": "double_quote"
        }
      ]
    },
    "single_quote": {
      "type": "STRING",
      "value": "'"
    },
    "double_quote": {
      "type": "STRING",
      "value": "\""
    },
    "symbols": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "PATTERN",
        "value": "[@#\\$%\\^\\&\\*\\(\\)_\\+\\=\\-/><~\\\\]"
      }
    },
    "literal": {
      "type": "PREC",
      "value": 10,
      "content": {
        "type": "PATTERN",
        "value": "\\\\[@#\\$%\\^\\&\\*\\(\\)_\\+\\=\\-/><~\\\\]"
      }
    },
    "content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      }
    },
    "_section": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "heading"
              },
              {
                "type": "SYMBOL",
                "name": "content"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "heading"
          },
          {
            "type": "SYMBOL",
            "name": "content"
          }
        ]
      }
    },
    "heading": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "heading_1"
        },
        {
          "type": "SYMBOL",
          "name": "heading_2"
        },
        {
          "type": "SYMBOL",
          "name": "heading_3"
        },
        {
          "type": "SYMBOL",
          "name": "heading_4"
        },
        {
          "type": "SYMBOL",
          "name": "heading_5"
        },
        {
          "type": "SYMBOL",
          "name": "heading_6"
        }
      ]
    },
    "heading_1": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "heading_2": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "heading_3": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "###"
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "heading_4": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "####"
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "heading_5": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#####"
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "heading_6": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "######"
        },
        {
          "type": "SYMBOL",
          "name": "_line"
        }
      ]
    },
    "emph": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "emph_start"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "word"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "puncuation"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "literal"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "symbols"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "line_break"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "line_end"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "word"
              },
              {
                "type": "SYMBOL",
                "name": "puncuation"
              },
              {
                "type": "SYMBOL",
                "name": "literal"
              },
              {
                "type": "PATTERN",
                "value": "[\\[\\]\\\\#\\$\\^\\&\\(\\)\\+\\=\\-/><]"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "emph_end"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [
    [
      "emph"
    ]
  ],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "line_end"
    },
    {
      "type": "SYMBOL",
      "name": "emph_start"
    },
    {
      "type": "SYMBOL",
      "name": "emph_end"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}