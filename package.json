{
  "name": "@jtlandis/tree-sitter-quarto2",
  "version": "0.1.0",
  "description": "quarto grammar for tree-sitter",
  "repository": "github:jtlandis/tree-sitter-quarto2",
  "author": "jtlandis (https://github.com/jtlandis)",
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "markdown"
  ],
  "files": [
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "grammar.js",
    "queries/*",
    "src/**"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "test": "node scripts/test.js",
    "build": "node scripts/build.js",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "publishConfig": {
    "access": "public"
  },
  "tree-sitter": [
    {
      "scope": "source.qmd",
      "injection-regex": "^(quarto|qmd)$",
      "file-types": [
        "qmd"
      ]
    }
  ]
}
