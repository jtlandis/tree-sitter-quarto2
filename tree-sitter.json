{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "quarto",
      "camelcase": "Quarto",
      "title": "Quarto",
      "scope": "source.qmd",
      "file-types": ["qmd"],
      "injection-regex": "^quarto$",
      "class-name": "TreeSitterQuarto"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Quarto markdown format",
    "authors": [
      {
        "name": "jtlandis",
        "email": "jtlandis314@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/jtlandis/tree-sitter-quarto2"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
